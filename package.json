{
  "name": "@leapjs/core",
  "author": "Sanchan Moses",
  "description": "Lightweight, fast, powerful node.js web framework",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WekanCompany/leapjs"
  },
  "homepage": "https://github.com/WekanCompany/leapjs",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "watch": "lerna run watch --parallel --stream",
    "build": "lerna run build --stream",
    "lint": "concurrently 'npm run lint:packages' 'npm run lint:integration' 'npm run lint:spec'",
    "lint:packages": "eslint 'packages/**/**.ts'",
    "lint:integration": "eslint 'integration/**.ts'",
    "lint:spec": "eslint 'packages/**/test/**.ts'",
    "prerelease": "./node_modules/.bin/lerna version --conventional-commits --exact --no-push --yes -m 'chore(release): publish %s'",
    "release": "./node_modules/.bin/lerna publish from-package --yes"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "@typegoose/typegoose": "6.4.0",
    "chalk": "^3.0.0",
    "class-validator": "^0.11.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.8.1",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/cache-manager": "^2.10.2",
    "@types/chai": "^4.2.10",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.5.34",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "chai": "^4.2.0",
    "concurrently": "5.1.0",
    "coveralls": "3.0.9",
    "cross-env": "7.0.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-import-resolver-lerna": "1.1.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.1",
    "lerna": "^3.20.2",
    "mocha": "7.0.1",
    "mocha-lcov-reporter": "1.3.0",
    "mochawesome": "4.1.0",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "shx": "^0.3.2",
    "source-map-support": "0.5.16",
    "ts-node": "8.6.2",
    "tsc-watch": "^4.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.3"
  },
  "version": "0.2.0"
}
