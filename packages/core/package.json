{
  "name": "@leapjs/core",
  "version": "0.1.0",
  "description": "",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "shx rm -rf build",
    "start": "cross-env NODE_ENV=development node build/src/index.js --color",
    "start:dev": "cross-env NODE_ENV=development node build/src/index.js --color",
    "watch": "tsc-watch --onSuccess \"npm run start:dev\"",
    "start:debug": "node --inspect-brk build/src/index.js --color",
    "start:prod": "cross-env NODE_ENV=production node build/src/index.js --color",
    "test": "cross-env NODE_ENV=test TS_NODE_FILES=true nyc mocha"
  },
  "dependencies": {
    "@leapjs/common": "^0.1.0",
    "esprima": "^4.0.1"
  },
  "devDependencies": {
    "@types/esprima": "^4.0.2",
    "@types/multer": "^1.4.2"
  }
}
