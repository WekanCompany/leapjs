  },
  "dependencies": {
    "@leapjs/common": "^0.1.0",
    "@typegoose/typegoose": "6.4.0",
    "cron": "1.7.2",
    "cron-parser": "2.13.0",
    "moment": "^2.24.0",
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/cron": "1.7.1"
  }
